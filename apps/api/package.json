{
  "name": "@roman-numeral-converter/api",
  "version": "1.0.0",
  "description": "API for the Roman Numeral Converter",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev -p 8080 --turbopack",
    "build": "tsc -b && next build",
    "start": "next start -p 8080",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.57.1",
    "@opentelemetry/exporter-prometheus": "^0.57.1",
    "@opentelemetry/instrumentation": "^0.57.1",
    "@opentelemetry/instrumentation-runtime-node": "^0.12.1",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-logs": "^0.57.1",
    "@opentelemetry/sdk-metrics": "^1.30.1",
    "@opentelemetry/semantic-conventions": "^1.29.0",
    "@roman-numeral-converter/messages": "workspace:*",
    "@vercel/otel": "^1.10.1",
    "next": "15.1.6",
    "pino": "^9.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "typescript": "^5",
    "vitest": "^3.0.5"
  },
